{
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "parser": "babel-eslint",
  "env": {
    "node": true
  },
  "rules": {
    "switch-colon-spacing": 0,
    "valid-jsdoc": 0,
    "max-len": 0,
    "no-console": 0,
    "no-undef": 0,
    "no-invalid-this": 0,
    "object-curly-spacing": [
      "error",
      "always"
    ]
  }
}
